{"hidden":true,"children":[{"configs":{"itemId":"module","serverScript":"var i = 10,\n  params = [],\n  roleId, roleName;\n\nif (app.has('roleId'))\n  roleId = app.getObject('roleId');\nif (app.has('roleName'))\n  roleName = app.getObject('roleName');\nWb.each(roleId || roleName, function(id) {\n  params.push('{?f' + i + '?}');\n  app.set('f' + i, id);\n  i++;\n});\napp.output('select * from WB_ROLE where ' + (roleId ? 'ROLE_ID' : 'ROLE_NAME') + ' in (' + params.join(',') + ')');"},"expanded":false,"children":[],"type":"module"}],"roles":{},"title":"查询指定的角色","iconCls":"","inframe":false,"pageLink":""}