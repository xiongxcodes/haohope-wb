{"hidden":false,"children":[{"configs":{"itemId":"module","serverScript":"var userIds = [],\n  recs = Wb.decode(request.getParameter('grid1'));\n\nWb.each(recs, function(rec) {\n  if (rec.USER_ID == 'admin')\n    Wb.error('不能禁用默认管理员帐户。');\n  userIds.push(rec.USER_ID);\n});\napp.run('update WB_USER set STATUS={?newStatus?} where USER_ID={?USER_ID?}', {\n  arrayName: 'grid1'\n});\n//移除会话\ncom.wb.common.UserList.invalidate(userIds);"},"expanded":true,"children":[],"type":"module"}],"roles":{},"title":"设置状态","iconCls":"","inframe":false,"pageLink":""}