{"hidden":true,"children":[{"configs":{"serverScript":"var data = app.get(),\n  whereSql = [];\nif (data.fromDate)\n  whereSql.push('LOG_DATE>={?timestamp.fromDate?}');\nif (data.toDate)\n  whereSql.push('LOG_DATE<={?timestamp.toDate?}');\nif (data.userName)\n  whereSql.push('USER_NAME={?userName?}');\nif (data.ip)\n  whereSql.push('IP={?ip?}');\nif (data.logType)\n  whereSql.push('LOG_TYPE={?integer.logType?}');\nif (data.msg) {\n  request.setAttribute('msg', '%' + data.msg + '%');\n  whereSql.push('MSG like {?msg?}');\n}\nif (whereSql.length)\n  whereSql = ' where ' + whereSql.join(' and ');\nelse\n  whereSql = '';\napp.output('select * from WB_LOG ' + whereSql + DbUtil.getOrderBySql(request, null), {\n  dictTableNames: 'WB_LOG'\n});","itemId":"module"},"expanded":true,"children":[],"type":"module"}],"roles":{},"title":"查询","iconCls":"","inframe":false,"pageLink":""}