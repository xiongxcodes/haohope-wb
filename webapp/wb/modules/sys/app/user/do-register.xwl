{"hidden":true,"children":[{"configs":{"itemId":"module","serverScript":"if (!Var.getBool('sys.app.account.allowRegister'))\n  SysUtil.accessDenied(request);\nvar session = request.getSession(false),\n  vc = session.getAttribute('register'),\n  password = app.get('PASSWORD');\nif (Wb.isEmpty(vc) || vc.toLowerCase() !== app.get('verifyCode').toLowerCase())\n  throw '验证码无效';\nif (password.length < 6)\n  throw '密码不能少于6位';\napp.set({\n  USER_ID: SysUtil.getId(),\n  STATUS: 1,\n  CREATE_DATE: new JavaDate(),\n  PASSWORD: Encrypter.getMD5(app.get('PASSWORD')),\n  LOGIN_TIMES: 0,\n  ROLE_ID: Var.get('sys.app.account.registerUserRole')\n});\napp.update({\n  tableName: 'WB_USER',\n  mode: 'insert'\n});\napp.update({\n  tableName: 'WB_USER_ROLE',\n  mode: 'insert'\n});","loginRequired":"false"},"expanded":false,"children":[],"type":"module"}],"roles":{"default":1},"title":"执行注册操作","iconCls":"","inframe":false,"pageLink":""}