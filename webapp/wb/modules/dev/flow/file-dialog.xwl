{"hidden":true,"children":[{"configs":{"itemId":"module","serverScript":"app.set('basePath', FileUtil.getPath(new File(Base.path, 'wb/system/resource')));","internalCall":"true"},"expanded":false,"children":[],"type":"module","events":{"initialize":"Wb.apply(app, {\n  addFile: function(callback, extName, filename) {\n    app.show(callback, extName, filename, true, {\n      iconCls: 'new_icon',\n      title: '新建'\n    });\n  },\n  saveAs: function(callback, extName, filename) {\n    app.show(callback, extName, filename, true, {\n      iconCls: 'save_as_icon',\n      title: '另存为'\n    });\n  },\n  save: function(callback, extName, filename) {\n    app.show(callback, extName, filename, true);\n  },\n  open: function(callback, extName, filename) {\n    app.show(callback, extName, filename);\n  },\n  show: function(callback, extName, filename, isSave, config) {\n    var params = {\n      extName: extName,\n      path: filename,\n      isSave: isSave,\n      multiSelect: !isSave\n    };\n    if (config)\n      Wb.apply(params, config);\n    Wb.run({\n      url: 'open-dialog',\n      single: true,\n      params: {\n        basePath: '{#basePath#}',\n        rootConfig: Wb.encode({\n          text: '全部'\n        }),\n        getFolderTreeUrl: 'm?xwl=dev/flow/get-files&type=1',\n        getFilesUrl: 'm?xwl=dev/flow/get-files&type=2',\n        addFileUrl: 'm?xwl=dev/flow/add-file',\n        renameUrl: 'm?xwl=dev/flow/rename',\n        deleteUrl: 'm?xwl=dev/flow/delete',\n        pasteUrl: 'm?xwl=dev/flow/paste',\n        getFileIconUrl: 'm?xwl=dev/flow/get-file-icon',\n        downloadUrl: 'm?xwl=dev/flow/download',\n        uploadUrl: 'm?xwl=dev/flow/upload',\n        forceShowFolder: true\n      },\n      success: function(scope) {\n        scope.show(callback, params);\n      }\n    });\n  }\n});"}}],"roles":{},"title":"文件对话框","iconCls":"","inframe":false,"pageLink":""}