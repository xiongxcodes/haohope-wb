{"hidden":true,"children":[{"configs":{"itemId":"module","serverScript":"var rows, objectName, result = {\n    'permx1': [],\n    'permx2': [],\n    'permx3': []\n  },\n  type = app.get('type');\n\nif (type == 'db')\n  objectName = app.get('jndi');\nelse\n  objectName = app.get('jndi') + '.' + app.get('prefix') + '.' + app.get(type);\napp.set('objectName', objectName);\nrows = app.getRecords('select PERM_TYPE,ROLE_NAME from WB_DBM where OBJECT_NAME={?objectName?}');\nWb.each(rows, function(row) {\n  result['permx' + row.PERM_TYPE].push(row.ROLE_NAME);\n});\napp.send(result);"},"expanded":true,"children":[],"type":"module"}],"roles":{},"title":"查询角色","iconCls":"","inframe":false,"pageLink":""}