{"hidden":true,"children":[{"configs":{"serverScript":"var keyName = app.get('KEY_NAME'),\n  keyId = SysUtil.getId();\nif (app.getInt('TYPE') == 3) {\n  app.run('select 1 from WB_KEY_TREE where KEY_NAME={?KEY_NAME?} and PARENT_ID={?PARENT_ID?}', {\n    errorText: '目录 \u201c' + keyName + '\u201d 已经存在。'\n  });\n} else {\n  app.run('select 1 from WB_KEY_TREE where KEY_NAME={?KEY_NAME?} and TYPE<>3', {\n    errorText: '键名 \u201c' + keyName + '\u201d 已经存在。'\n  });\n}\napp.set('KEY_ID', keyId);\napp.update({\n  tableName: 'WB_KEY_TREE',\n  mode: 'insert'\n});\n//把更新的记录值返回到前台\napp.output('select * from WB_KEY_TREE where KEY_ID={?KEY_ID?}', {\n  type: 'object'\n});","itemId":"module"},"expanded":true,"children":[],"type":"module"}],"roles":{},"title":"添加","iconCls":"","inframe":false,"pageLink":""}