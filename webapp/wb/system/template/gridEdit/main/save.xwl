{"hidden":false,"children":[{"configs":{"serverScript":"var id,\n  create = Wb.decode(app.get('create'));\n//重新设置需要添加的记录列表值\nWb.each(create, function(rec) {\n  if(!rec.{#keyField#}){\n    id = SysUtil.getId();\n    rec.{#keyField#} = id; //设置id值\n  }\n});\nrequest.setAttribute('create', Wb.encode(create));\n//把需要增删改的记录同步到数据库表\napp.update({\n  tableName: '{#tableName#}'\n});\n//把需要同步的记录返回到前台\napp.send({\n  create: create\n});","itemId":"module"},"expanded":true,"children":[],"type":"module"}],"roles":{"default":1},"title":"保存","iconCls":"","inframe":false,"pageLink":""}