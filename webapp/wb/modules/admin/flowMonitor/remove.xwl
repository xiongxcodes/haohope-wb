{"hidden":false,"children":[{"configs":{"itemId":"module","serverScript":"var grid1 = Wb.decode(app.get('grid1')),\n  flowIds = Wb.unique(Wb.pluck(grid1, 'FLOW_ID')),\n  flowIdRows = [];\nWb.each(flowIds, function(id) {\n  flowIdRows.push({\n    FLOW_ID: id\n  });\n});\n//flowIdRows为唯一化的流程id列表\napp.set('flowIdRows', Wb.encode(flowIdRows));\n//指定arrayData自动启用事务\napp.run('delete from WB_FLOW where FLOW_ID={?FLOW_ID?}', {\n  arrayData: flowIdRows\n});\napp.run('delete from WB_FLOW_USER where FLOW_ID={?FLOW_ID?}', {\n  arrayData: flowIdRows\n});\napp.run('delete from WB_FLOW_LIST where FLOW_ID={?FLOW_ID?}', {\n  arrayData: grid1\n});"},"expanded":true,"children":[],"type":"module"}],"roles":{},"title":"删除","iconCls":"","inframe":false,"pageLink":""}